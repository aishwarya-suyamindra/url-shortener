{
  "testEnvironment": "node",
  "collectCoverage": true,
  "coverageDirectory": "coverage/unit",
  "testMatch": ["**/*.test.{js,ts}"],
  "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|ts)$"
  ],
  "transform": {
      "^.+\\.js$": "babel-jest"
  },
  "coveragePathIgnorePatterns": ["/node_modules/"]
}
